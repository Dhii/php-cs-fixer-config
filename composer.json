{
    "name": "dhii/php-cs-fixer-config",
    "description": "A default PHP CS Fixer config implementation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "friendsofphp/php-cs-fixer": ">=1.11 <1.12",
        "php": "^5.4 | ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Configuration\\PHPCSFixer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\Configuration\\PHPCSFixer\\UnitTest\\": "test/unit"
        }
    }
}
